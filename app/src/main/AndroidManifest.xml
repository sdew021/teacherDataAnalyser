<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dataanalyser">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="Digilyzer"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".pdf">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".aboutus"/>

        <activity android:name=".homePage" />
        <activity android:name=".finalanalysis" />
        <activity android:name=".addActivity" />
        <activity android:name=".dataEntry" />
        <activity android:name=".selectSubject" />
        <activity android:name=".subjectList" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>